<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

    <application>
        <el-resolver>
            org.primefaces.application.exceptionhandler.PrimeExceptionHandlerELResolver
        </el-resolver>
    </application>
    <factory>
        <exception-handler-factory>
            org.primefaces.application.exceptionhandler.PrimeExceptionHandlerFactory
        </exception-handler-factory>
    </factory>
    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cadAluno</from-outcome>
            <to-view-id>/caddeAluno.xhtml</to-view-id>
        </navigation-case>  
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>cadPersonal</from-outcome>
            <to-view-id>/caddeProfessor.xhtml</to-view-id>
        </navigation-case>  
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>listaAluno</from-outcome>
            <to-view-id>/listadeAlunos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>listaProfessor</from-outcome>
            <to-view-id>/listadeProfessor.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loginProfessor</from-outcome>
            <to-view-id>/loginProfessor.xhtml</to-view-id>
        </navigation-case>  
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loginAluno</from-outcome>
            <to-view-id>/loginAluno.xhtml</to-view-id>
        </navigation-case>  
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>listaProfessor</from-outcome>
            <to-view-id>/listadeProfessor.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addAluno</from-outcome>
            <to-view-id>/addAlunosPorPersonal.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    
    
    <navigation-rule>
        <from-view-id>/listadeAlunos.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pagInicial</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/listadeProfessor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pagInicial</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
   
    
    
    <navigation-rule>
        <from-view-id>/caddeAluno.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pagInicial</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/caddeProfessor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pagInicial</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    <navigation-rule>
        <from-view-id>/indexProfessor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addAlunoPersonal</from-outcome>
            <to-view-id>/addAlunosPorPersonal.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
  <navigation-rule>
        <from-view-id>/indexProfessor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addExercicioAluno</from-outcome>
            <to-view-id>/addExercicioAluno.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/indexProfessor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>meusAlunos</from-outcome>
            <to-view-id>/meusAlunos.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/indexProfessor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pagIndexInicial</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/addAlunosPorPersonal.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addExercicioAluno</from-outcome>
            <to-view-id>/addExercicioAluno.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/addAlunosPorPersonal.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pagInicial</from-outcome>
            <to-view-id>/indexProfessor.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/indexProfessor.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Logout</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/indexAluno.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>pagIndexInicial</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    
    
    
</faces-config>
